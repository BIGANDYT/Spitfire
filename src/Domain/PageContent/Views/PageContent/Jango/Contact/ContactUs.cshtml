@using Habitat.Identity
@using Habitat.Identity.Repositories
@model Sitecore.Mvc.Presentation.RenderingModel
@{
    var identity = IdentityRepository.Get(Model.Item);
    if (identity == null)
    {
        return;
    }
}
<!-- BEGIN: CONTENT/CONTACT/CONTACT-1 -->
<div class="c-content-box c-size-md c-bg-img-top c-no-padding c-pos-relative">
    <div class="container">
        <div class="c-content-contact-1 c-opt-1">
            <div class="row" data-auto-height=".c-height">
                <div class="col-sm-8 c-desktop"></div>
                <div class="col-sm-4">
                    <div class="c-body">
                        <div class="c-section">
                            @Html.Sitecore().Field("Logo", identity)
                        </div>
                        <div class="c-section">
                            <h3>@Html.Sitecore().Field("Name", identity)</h3>
                        </div>
                        <div class="c-section">
                            <div class="c-content-label c-font-uppercase c-font-bold c-theme-bg">Address</div>
                            <p>
                                @Html.Sitecore().Field("Address Line 1", identity)
                                <br>@Html.Sitecore().Field("County", identity)
                                <br>@Html.Sitecore().Field("Country", identity)
                                <br>@Html.Sitecore().Field("Postcode", identity)
                            </p>
                        </div>
                        <div class="c-section">
                            <div class="c-content-label c-font-uppercase c-font-bold c-theme-bg">Contacts</div>
                            <p>
                                <strong>T</strong> @Html.Sitecore().Field("Home Phone", identity)
                                <br>
                                <strong>F</strong> @Html.Sitecore().Field("Fax", identity)
                            </p>
                        </div>
                        <div class="c-section">
                            <div class="c-content-label c-font-uppercase c-font-bold c-theme-bg">Social</div>
                            <br>
                            <ul class="c-content-iconlist-1 c-theme">
                                <li>
                                    @Html.Sitecore().BeginField("Twitter", Model.Item, new { haschildren = true })
                                    <i class="fa fa-twitter"></i>
                                    @Html.Sitecore().EndField()
                                </li>
                                <li>
                                    @Html.Sitecore().BeginField("Facebook", Model.Item, new { haschildren = true })
                                    <i class="fa fa-facebook"></i>
                                    @Html.Sitecore().EndField()
                                </li>
                                <li>
                                    @Html.Sitecore().BeginField("Youtube", Model.Item, new { haschildren = true })
                                    <i class="fa fa-youtube-play"></i>
                                    @Html.Sitecore().EndField()
                                </li>
                                <li>
                                    @Html.Sitecore().BeginField("LinkedIn", Model.Item, new { haschildren = true })
                                    <i class="fa fa-linkedin"></i>
                                    @Html.Sitecore().EndField()
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
